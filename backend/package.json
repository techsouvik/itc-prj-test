{
  "name": "azure-devops-sprint-manager-backend",
  "version": "1.0.0",
  "description": "TypeScript Express backend for Azure DevOps Sprint Manager with AI integration",
  "main": "dist/main.server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.server.ts",
    "build": "tsc",
    "start": "node dist/main.server.js",
    "mcp": "node dist/mcp/server.js",
    "build:mcp": "tsc && chmod +x dist/mcp/server.js",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "azure-devops",
    "sprint-manager",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "socket.io": "^4.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  }
}
